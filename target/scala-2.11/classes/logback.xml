<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{YYYY-MM-dd HH:mm:ss} [%thread] [%logger{0}] [%-5level] %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="PageViewProcess" level="WARN"/>

    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>